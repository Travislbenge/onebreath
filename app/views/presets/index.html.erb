<div class="container text-center">
  <div class="row">
    <div class="col-xs-12 col-md-6 col-md-offset-3">
      <div class="title-box">
        <h1 class="h1title">Timer Flows</h1>
      </div>
    </div>
  </div>
    <div class="col-xs-10 col-xs-offset-1 col-md-6 col-md-offset-3">
      <div class="sub-title-box">
        <p class="pbold">

          Create your new timer preset flow or select one.

        </p>
      </div>
    </div>
  <div class="col-xs-8 col-xs-offset-2 col-md-4 col-md-offset-4">
    <div class="middle-content">
     <% @presets.each do |preset| %>
     <div class="presets-box">
       <%= link_to preset.name, preset_timer_sessions_path(preset), method: :post, :class=> "btn obbtn flow-unselected"%>
       <br>
       <%= link_to 'delete', preset , method: :delete,
       data: { confirm: "Are you sure?" }, id: ' @preset.id ' %>
       || <%= link_to 'edit', edit_preset_path(preset)%>
       <% end %>
     </div>
     <div class="presets-box">
      <%= link_to "Add new", new_preset_path, :class=> "btn obbtn sign-in" %>
    </div>
  </div>
</div>
</div>
<div class="bottom-blank">
</div>
